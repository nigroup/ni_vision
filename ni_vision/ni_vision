#!/bin/bash
rosrun ni_vision ni_vision ~input_img:=/camera/rgb/image_color ~input_pc:=/camera/depth_registered/points -dlim 5 -ddmod 0 -dgc 0.5 -dgfmod 2 -dgfsize 5 -dgsmtmod 2 -dgtauf1 0.0018 -dgtauf2 0.0045 -dstaud 0.04 -dstaug 0.003 -dsgcs 10 -dstaus 200 -gssigma 0.8 -gsgth 200 -gsmins 150 -trkmod 0 -trkdp 0.15 -trkds 0.3 -trkdc 0.3 -trkd 1.6 -trkpf 0.1 -trksf 0.5 -trkcf 0.4 -trkmf 100 -trkcmod 1 -trkcm 10 -trkcs 1 -trkcd 0 -atttd 0 -promax 400 -promin 100 -propm 200 -proar1 20 -proar2 30 -siftsc 3 -siftis 1.6 -siftpt 0.015 -flknn 2 -fltp 0.9 -flmf 0.70 -flmc 3 -flmcw 20 -recfeat 20 -recdc 0.45 -reccmm 1 -recvmd 0 -recordmod 1 -tmessfl 500 -snapfm 1 -target DanKlorix -datadir ~/zNiData -libpath ~/Video/library/Versuchslib4B/ -siftlibfile lib_sift_Pringles_0015.yaml -colorlibfile simplelib_3dch_Pringles.yaml -nnthresh 0.2 -trovl 0.8 -deltabin 12 -tnumb 3 -tscale 0.1 -torient 0.5233333
